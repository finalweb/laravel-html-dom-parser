{
    "name": "ammadeuss/laravel-html-dom-parser",
    "description": "Laravel wrapper for the PHP Simple HTML DOM Parser package",
    "keywords": [
        "dom parser",
        "html dom parser",
        "laravel"
    ],
    "require": {
    	"php": ">=8.0",
        "illuminate/support": ">=8.0",
        "kub-at/php-simple-html-dom-parser": "^1.8"
    },
    "homepage": "https://github.com/ammadeuss/html-dom-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Ammadeuss Raducan",
            "email": "raducan@ammadeuss.ro"
        }
    ],
    "autoload": {
        "psr-4": { "Ammadeuss\\LaravelHtmlDomParser\\": "" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ammadeuss\\LaravelHtmlDomParser\\ServiceProvider"
            ],
            "aliases": {
                "HTMLDomParser": "Ammadeuss\\LaravelHtmlDomParser\\Facade"
            }
        }
    }
}
